{% import 'Text_Utilities.jinja' as Utils -%}

{% for uuid, class in SerializeContext.items()|sort(attribute='1.Name') %}
    {% if class.IsSystemComponent %}
{{- Utils.PrintName(uuid, class) }}
{{- Utils.PrintClassContent(SerializeContext, 2, uuid) -}}
{{- safe }}
    {% endif %}
{% endfor %}