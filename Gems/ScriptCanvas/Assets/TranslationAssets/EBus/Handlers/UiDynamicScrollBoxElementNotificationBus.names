                        {
                            "typeid": "{72039442-EB38-4D42-A1AD-CB68F7E0EEF6}",
                            "details": {
                                "name": "Element Index",
                                "tooltip": "The index of the child that is being prepared"
                            }
                        }
                    ]
                },
                {
