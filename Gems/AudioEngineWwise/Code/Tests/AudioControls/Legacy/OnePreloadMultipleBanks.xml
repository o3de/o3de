  <ATLTrigger atl_name="test_trigger"/>
 </AudioTriggers>
 <AudioPreloads>
  <ATLPreloadRequest atl_name="preload" atl_type="AutoLoad">
   <ATLPlatforms>
    <Platform atl_name="windows" atl_config_group_name="default"/>
    <Platform atl_name="mac" atl_config_group_name="default"/>
    <Platform atl_name="linux" atl_config_group_name="default"/>
    <Platform atl_name="android" atl_config_group_name="default"/>
    <Platform atl_name="ios" atl_config_group_name="default"/>
    <Platform atl_name="appletv" atl_config_group_name="default"/>
    <Platform atl_name="provo" atl_config_group_name="default"/>
    <Platform atl_name="salem" atl_config_group_name="default"/>
   </ATLPlatforms>
   <ATLConfigGroup atl_name="default">
    <WwiseFile wwise_name="test_bank1.bnk"/>
    <WwiseFile wwise_name="test_bank2.bnk"/>
   </ATLConfigGroup>
  </ATLPreloadRequest>
 </AudioPreloads>
</ATLConfig>
