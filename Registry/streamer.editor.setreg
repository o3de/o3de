{
    "Amazon":
    {
        "AzCore":
        {
            "Streamer":
            {
                "Profiles":
                {
                    "Generic":
                    {
                        "Stack":
                        {
                            "Remote drive":
                            {
                                "$type": "AzFramework::RemoteStorageDriveConfig",
                                "$stack_after": "Drive",
                                // The maximum number of file handles that the drive will cache.
                                "MaxFileHandles": 1024
                            }
                        }
                    }
                }
            }
        }
    }
}
