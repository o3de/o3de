{
    "Amazon": {
        "AssetProcessor": {
            "Settings": {
                "Platform pc": {
                    "tags": "tools,renderer"
                },
                "Platform mac": {
                    "tags": "tools,renderer"
                },
                "Platforms": {
                    "android": "enabled"
                },
                "ScanFolder Game": {
                    "watch": "@PROJECTROOT@",
                    "recursive": 1,
                    "order": 0
                },
                "RC i_caf": {
                    "glob": "*.i_caf",
                    "params": "copy",
                    "priority": 5
                }
            }
        }
    }
}
